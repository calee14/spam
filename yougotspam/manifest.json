{
  "manifest_version": 2,
  "name": "You Got Spam",
  "version": "0.60",
  "content_scripts": [
    {
      "matches": ["*://*.stackoverflow.com/*", "https://mail.google.com/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
  	"default_icon": "icon.png"
	},
  "permissions": ["tabs", 
    "activeTab", 
    "identity",
    "<all_urls>",
    "webNavigation"],
	"background": {
  	"scripts": ["background.js", "jquery-3.2.1.min.js"]
	},
  "web_accessible_resources": [
    "index.html",
    "popup.js"
  ]
}